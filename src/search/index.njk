<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="{{ "/css/nhs-toolkit.min.css" | url }}">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/normalize/8.0.1/normalize.min.css">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

        <script src={{ "/js/search_data.js" | url }}></script>
        <script src={{ "/js/tipuesearch_set.js" | url }}></script>
        <script src={{ "/js/tipuesearch.min.js" | url }}></script>
        <title>Search</title>
    </head>
    <body>
        <form>
            <div class="tipue_search_group">
                <input type="text" name="q" id="tipue_search_input" pattern=".{3,}" title="At least 3 characters" required>
                <button type="submit" class="tipue_search_button">
                  search
                </button>
            </div>
        </form>

        <div id="tipue_search_content"></div>
        <script>
            $(document).ready(function () {
                $('#tipue_search_input').tipuesearch({ showURL: false, imageZoom: false });
            });
        </script>
    </body>
</html>